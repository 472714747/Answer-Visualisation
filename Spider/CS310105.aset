						public SqRootOfStacks(Stack s){
						if (s == null)
						throw IllegalArgumentException(&quot; cant have null stack&quot;);
						if (s.size &lt;=0)
						throw IllegalArgumentException(&quot; stack must have values in it&quot;);
						stack = s;
						
						/* Post-condition*/
						assert (stack.equals(s));
